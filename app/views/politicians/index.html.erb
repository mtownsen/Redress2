<table class="table table-striped">
  <thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Contact Form</th>
  </tr>
  </thead>

  <tbody>
  <% @legislators.each do |legislator| %>

      <tr>
        <td><%= legislator["first_name"] %></td>
        <td><%= legislator["last_name"] %></td>
        <td><%= legislator["contact_form"] %></td>
      </tr>
  <% end %>
  </tbody>
</table>

